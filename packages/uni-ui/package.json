{
  "name": "@vingogo/uni-ui",
  "version": "0.2.7",
  "private": false,
  "license": "MIT",
  "author": "Alvin Liu",
  "description": "基于 Vue 3 的轻量、快速的多平台开发 UI 组件库",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "style": "lib/style.css",
  "typings": "lib/types/components.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "cross-env MODE=play node build/index.js && cross-env MODE=play node ./scripts/watcher.js",
    "build:all": "npm run build:lib && npm run build:uni",
    "build:lib": "cross-env MODE=production node build/index.js",
    "build:uni": "cross-env MODE=UNI node build/uni.js",
    "dts": "rimraf types && vue-tsc --declaration --emitDeclarationOnly",
    "type-check": "vue-tsc --noEmit"
  },
  "sideEffects": [
    "lib/components/**/style.mjs",
    "*.scss",
    "*.css"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vingogo/vin-ui.git",
    "directory": "src/vingogo/uni-ui"
  },
  "keywords": [
    "vue",
    "vue3",
    "uniapp",
    "vin-ui",
    "vue3 component"
  ],
  "files": [
    "lib",
    "types"
  ],
  "bugs": {
    "url": "https://github.com/vingogo/vin-ui/issues"
  },
  "homepage": "https://github.com/vingogo/vin-ui/tree/main/src/vingogo/uni-ui#readme",
  "peerDependencies": {
    "vue": "^3.2.45",
    "vue-i18n": "^9.1.9"
  }
}
