{
  "name": "vingogo",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vingogo/vin-ui.git"
  },
  "scripts": {
    "dev": "vin dev --port 3001",
    "build": "vin build",
    "lint": "eslint --ext .ts,.js,.vue --fix .",
    "format": "prettier --write --cache --parser typescript \"**/*.[tj]s?(x)\"",
    "prepare": "husky install",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build",
    "build:lib": "node build/index.js",
    "deploy:ui": "npm run build:h5 && node ./scripts/deploy.js --prefix=ui --dist=dist/build/h5",
    "deploy:docs": "npm run docs:build && node ./scripts/deploy.js --prefix=docs --dist=docs/.vuepress/dist"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-alpha-3070420230224002",
    "@dcloudio/uni-app-plus": "3.0.0-alpha-3070420230224002",
    "@dcloudio/uni-components": "3.0.0-alpha-3070420230224002",
    "@dcloudio/uni-h5": "3.0.0-alpha-3070420230224002",
    "@dcloudio/uni-mp-alipay": "3.0.0-alpha-3070420230224002",
    "@dcloudio/uni-mp-baidu": "3.0.0-alpha-3070420230224002",
    "@dcloudio/uni-mp-kuaishou": "3.0.0-alpha-3070420230224002",
    "@dcloudio/uni-mp-lark": "3.0.0-alpha-3070420230224002",
    "@dcloudio/uni-mp-qq": "3.0.0-alpha-3070420230224002",
    "@dcloudio/uni-mp-toutiao": "3.0.0-alpha-3070420230224002",
    "@dcloudio/uni-mp-weixin": "3.0.0-alpha-3070420230224002",
    "vue": "^3.2.33"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "lint-staged": {
    "*.md": "prettier --write",
    "*.{js,json}": [
      "eslint",
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ],
    "**/*.{scss,css}": [
      "stylelint --fix",
      "prettier --parser css --write"
    ]
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@dcloudio/types": "^3.2.11",
    "@dcloudio/uni-automator": "3.0.0-alpha-3070420230224002",
    "@dcloudio/uni-cli-shared": "3.0.0-alpha-3070420230224002",
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-3070420230224002",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vingogo/vin-cli": "^0.3.1",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitest/ui": "^0.10.2",
    "@vue/test-utils": "^2.0.2",
    "@vue/vue3-jest": "^28.0.1",
    "ali-oss": "^6.17.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-import": "^1.13.5",
    "c8": "^7.11.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.0",
    "jest": "^26.6.3",
    "less": "^4.1.2",
    "minimist": "^1.2.8",
    "postcss": "^8.4.6",
    "postcss-preset-env": "^7.2.3",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "ts-jest": "^26.5.5",
    "typescript": "^4.6.3",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.6.0",
    "vitest": "^0.10.2",
    "vue-jest": "^5.0.0-alpha.7",
    "vue3-jest": "^27.0.0-alpha.1"
  },
  "peerDependencies": {
    "@dcloudio/vite-plugin-uni": "^3.0.0-alpha-3070420230224002"
  }
}
